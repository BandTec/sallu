        android:layout_width="230dp"
        android:layout_height="230dp"
        android:layout_marginRight="35dp"
        android:src="@drawable/foto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="16dp" />


    <TextView
        android:id="@+id/tv_data"
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:layout_marginLeft="250dp"
        android:text="16/10/2020"
        android:textColor="#000000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="58dp" />

    <EditText
        android:id="@+id/et_ficha1"
        android:layout_width="249px"
        android:layout_height="40dp"
        android:layout_marginBottom="600dp"
        android:background="#D9D9D9"
        android:paddingLeft="30dp"
        android:text="Hoje"
        android:textColor="#FC6963"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.86"
        app:layout_constraintStart_toStartOf="parent"
        />

    <CalendarView
        android:id="@+id/cv_calendario"
        android:layout_width="410dp"
        android:layout_height="100dp"
        android:outlineAmbientShadowColor="@color/blue"
        android:scrollbarSize="300dp"
        app:layout_constraintTop_toBottomOf="@id/iv_fotoConsulta"
        tools:layout_editor_absoluteX="1dp" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#000000"
        android:focusable="false"
        android:id="@+id/v_linha1"
        app:layout_constraintTop_toBottomOf="@id/cv_calendario"
        />

    <EditText
        android:id="@+id/et_coluna"
        android:layout_width="110dp"
        android:layout_height="390dp"
        android:background="#D9D9D9"
        app:layout_constraintTop_toBottomOf="@+id/cv_calendario"
        tools:layout_editor_absoluteX="0dp" />

