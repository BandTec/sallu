<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D9D9D9"
    tools:context=".HistoricoMedico" >

    <EditText
        android:layout_width="415dp"
        android:layout_height="200dp"
        android:background="#2FB186"
        android:id="@+id/et_cor"
        app:layout_constraintHorizontal_bias="0.495"
        android:layout_marginBottom="15dp"
        android:radius="200dp"
        app:layout_constraintVertical_bias="0.0"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="@string/txt_titulo_historico"
        android:textColor="#D9D9D9"
        android:textSize="35sp"
        android:layout_marginTop="100dp"
        android:textStyle="bold"
        android:layout_marginLeft="1dp"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/tv_configuracao"
        app:layout_constraintTop_toBottomOf="@+id/tv_usuario"/>

    <ImageView
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_marginLeft="90dp"
        android:id="@+id/iv_fotoHistorico"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/arquivo"
        />

    <TextView
        android:id="@+id/tv_1"
        android:layout_width="150dp"
        android:layout_height="32dp"
        android:layout_marginTop="300dp"
        android:paddingLeft="10dp"
        android:text="Nome Hospital"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/iv_fotoHistorico"
        tools:layout_editor_absoluteX="1dp" />

    <TextView
        android:id="@+id/tv_resposta1"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginTop="330dp"
        android:visibility="invisible"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_marginLeft="20dp"
        android:paddingLeft="10dp"
        app:layout_constraintTop_toTopOf="@+id/iv_fotoHistorico"
        android:textSize="15dp"
        />

    <TextView
        android:id="@+id/tv_2"
        android:layout_width="80dp"
        android:layout_height="32dp"
        android:layout_marginTop="300dp"
        android:paddingLeft="10dp"
        android:text="Telefone"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/iv_fotoHistorico"
        tools:layout_editor_absoluteX="130dp" />

    <TextView
        android:id="@+id/tv_resposta2"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:paddingLeft="10dp"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintTop_toTopOf="@+id/tv_2"
        tools:layout_editor_absoluteX="120dp" />

    <TextView
        android:id="@+id/tv_3"
        android:layout_width="80dp"
        android:layout_height="32dp"
        android:layout_marginTop="300dp"
        android:paddingLeft="10dp"
        android:text="Pressão"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/iv_fotoHistorico"
        tools:layout_editor_absoluteX="215dp" />

    <TextView
        android:id="@+id/tv_resposta3"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:paddingLeft="10dp"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintTop_toTopOf="@+id/tv_3"
        tools:layout_editor_absoluteX="210dp" />

    <TextView
        android:id="@+id/tv_4"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:layout_marginTop="300dp"
        android:paddingLeft="10dp"
        android:text="Temperatura"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/iv_fotoHistorico"
        tools:layout_editor_absoluteX="295dp" />

    <TextView
        android:id="@+id/tv_resposta4"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:paddingLeft="100dp"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintTop_toTopOf="@+id/tv_3"
        tools:layout_editor_absoluteX="305dp" />

    <TextView
        android:id="@+id/et_data"
        android:layout_width="160dp"
        android:layout_height="30dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/custom_today_box"
        android:paddingLeft="30dp"
        android:textColor="#FC6963"
        android:textSize="20sp"
        android:visibility="invisible"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/tv_4"
        tools:layout_editor_absoluteX="244dp" />


    <TextView
        android:id="@+id/tv_5"
        android:layout_width="150dp"
        android:layout_height="32dp"
        android:layout_marginTop="100dp"
        android:paddingLeft="10dp"
        android:text="Nome Hospital"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/tv_resposta1"
        tools:layout_editor_absoluteX="1dp" />

    <TextView
        android:id="@+id/tv_resposta5"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginTop="23dp"
        android:visibility="invisible"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_marginLeft="20dp"
        android:paddingLeft="10dp"
        app:layout_constraintTop_toTopOf="@+id/tv_5"
        android:textSize="15dp"
        />

    <TextView
        android:id="@+id/tv_6"
        android:layout_width="80dp"
        android:layout_height="32dp"
        android:paddingLeft="10dp"
        android:text="Telefone"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/tv_5"
        tools:layout_editor_absoluteX="130dp" />

    <TextView
        android:id="@+id/tv_resposta6"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:paddingLeft="10dp"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintTop_toTopOf="@+id/tv_6"
        tools:layout_editor_absoluteX="120dp" />

    <TextView
        android:id="@+id/tv_7"
        android:layout_width="80dp"
        android:layout_height="32dp"
        android:paddingLeft="10dp"
        android:text="Pressão"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/tv_6"
        tools:layout_editor_absoluteX="215dp" />

    <TextView
        android:id="@+id/tv_resposta7"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:paddingLeft="10dp"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintTop_toTopOf="@+id/tv_7"
        tools:layout_editor_absoluteX="210dp" />

    <TextView
        android:id="@+id/tv_8"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:paddingLeft="10dp"
        android:text="Temperatura"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/tv_7"
        tools:layout_editor_absoluteX="295dp" />

    <TextView
        android:id="@+id/tv_resposta8"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:paddingLeft="100dp"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintTop_toTopOf="@+id/tv_8"
        tools:layout_editor_absoluteX="305dp" />

    <TextView
        android:id="@+id/et_data2"
        android:layout_width="160dp"
        android:layout_height="30dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/custom_today_box"
        android:paddingLeft="30dp"
        android:textColor="#FC6963"
        android:textSize="20sp"
        android:visibility="invisible"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/tv_resposta8"
        tools:layout_editor_absoluteX="244dp" />

    <Button
        android:id="@+id/bt_configurar"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:layout_marginTop="348dp"
        android:background="@drawable/shape_login_buttom_custom"
        android:text="@string/txt_btn_visualizar"
        android:textAllCaps="false"
        android:textColor="#FFF"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="@+id/tv_1"
        tools:layout_editor_absoluteX="76dp" />


</androidx.constraintlayout.widget.ConstraintLayout>